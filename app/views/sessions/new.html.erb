<h1> Welcome Story Teller </h1>

<h3>Please Log In To Share Your Story</h3>

<%= form_with url: login_path do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %><br><br>
    <%= f.label :password %>
    <%= f.password_field :password %><br><br>
    <%= f.submit "Log In" %>
    <br><br>
    <%= link_to "Back", root_path %>
<% end%>

<h3> Or Log In With Google </h3>

<%= button_to 'Google', '/auth/google_oauth2' %>
